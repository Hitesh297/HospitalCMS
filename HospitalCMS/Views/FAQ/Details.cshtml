@model HospitalCMS.Models.FAQDto

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<div>
    <h4>FAQ</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Department Name
        </dt>

        <dd>
            @Model.Department.Name
        </dd>

        <dt>
            Question
        </dt>

        <dd>
            @Model.Question
        </dd>

        <dt>
            Answer
        </dt>

        <dd>
            @Model.Answer
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.FAQId }) |
    @Html.ActionLink("Back to List", "List")
</p>
